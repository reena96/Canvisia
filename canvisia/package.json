{
  "name": "canvisia",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "bash scripts/dev.sh",
    "dev:emulator": "firebase emulators:start --import=./.emulator-data --export-on-exit",
    "dev:with-emulators": "concurrently \"npm run dev:emulator\" \"npm run dev\"",
    "seed-users": "tsx scripts/seed-emulator-users.ts",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "emulators": "firebase emulators:start",
    "test:integration": "firebase emulators:exec --only auth,firestore,database 'vitest run tests/integration'"
  },
  "dependencies": {
    "firebase": "^12.4.0",
    "konva": "^10.0.2",
    "lucide-react": "^0.545.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-konva": "^19.0.10",
    "react-router-dom": "^7.9.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.7.2",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "firebase-admin": "^13.5.0",
    "firebase-tools": "^14.19.1",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "puppeteer": "^24.25.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
